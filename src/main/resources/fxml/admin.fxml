<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.AdminController">
    <TableView fx:id="userTable" layoutY="64.0" onMouseClicked="#handleMouseClicked" prefHeight="329.0" prefWidth="600.0">
      <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="201.0" text="Username" />
          <TableColumn fx:id="emailColumn" minWidth="0.0" prefWidth="247.0" text="Email" />
          <TableColumn fx:id="roleColumn" prefWidth="151.0" text="Role" />
      </columns>
    </TableView>
    <Button layoutX="445.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleNewAccountButton" prefHeight="26.0" prefWidth="122.0" text="New Account" />
   <Button layoutX="309.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleDeleteAccountButton" prefHeight="26.0" prefWidth="122.0" text="Delete" />
   <ImageView fx:id="LOGOutIMG" fitHeight="65.0" fitWidth="173.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@../logo/LOGO.png" />
   </ImageView>
    <stylesheets>
        <String fx:value="/Style.css" />
    </stylesheets>
</AnchorPane>
